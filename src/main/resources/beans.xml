<?xml version="1.0" encoding="UTF-8" ?>
<beans>
    <bean id="aservice" class="com.djk.minis.test.AServiceImpl">
        <constructor-arg type="String" name="name" value="abc"/>
        <constructor-arg type="int" name="level" value="3"/>
        <property type="String" name="property1" value="Someone says"/>
        <property type="String" name="property2" value="Hello World!"/>
        <property type="com.djk.minis.test.BaseService" name="ref1"
                  ref="baseservice"/>
    </bean>
    <bean id="basebaseservice" class="com.djk.minis.test.BaseBaseService">
        <property type="com.djk.minis.test.AServiceImpl" name="as" ref="aservice"/>
    </bean>
    <bean id="baseservice" class="com.djk.minis.test.BaseService">
        <property type="com.djk.minis.test.BaseBaseService" name="bbs"
                  ref="basebaseservice"/>
    </bean>
</beans>